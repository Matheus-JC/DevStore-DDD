@model IEnumerable<DevStore.Common.Data.EventSourcing.StoredEvent>

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    @{
        <table class="table table-sm table-bordered table-hover col-md-12">
            <thead class="table-dark">
                <tr>
                    <td>Id</td>
                    <td>Name</td>
                    <td>Data</td>
                    <td>JSON</td>
                </tr>
            </thead>
            <tbody>
                @foreach (var eventItem in Model)
                {
                    <tr>
                        <td>@eventItem.Id</td>
                        <td>@eventItem.Type</td>
                        <td>@eventItem.OccurrenceDate</td>
                        <td><code>@eventItem.Data</code></td>
                    </tr>
                }
            </tbody>
        </table>
    }
</div>
